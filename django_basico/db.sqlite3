CREATE Table Cadastro_empresa(
  id integer PRIMARY KEY AUTOINCREMENT,
  cnpj TEXT NOT NULL UNIQUE,
  razao_social text NOT NULL,
  nome_social text NOT NULL,
  area_atuacao text  NOT NULL,
  tempo_area text NOT null,
  capital_social numeric(10,2) not NULL,
  num_funcionario_clt integer not NULL,
  num_funcionario_3 integer not NULL,
  num_estagio integer NOT NULL,
  num_socio integer not NULL
)
